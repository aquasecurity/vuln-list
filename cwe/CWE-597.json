      "",
      "echo '($i === $s1) is FALSE'. \"\\n\";",
      "",
      "",
      "",
      "",
      "",
      "",
      "echo '($i == $s1) is TRUE'. \"\\n\";",
      "",
      "",
      "",
      "echo '($i == $s1) is FALSE'. \"\\n\";",
