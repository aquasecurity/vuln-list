{
  "id": "ALAS-2015-584",
  "title": "Amazon Linux AMI 2014.03 - ALAS-2015-584: medium priority package update for php55",
  "issued": {
    "date": "2015-08-17 12:41"
  },
  "updated": {
    "date": "2016-03-16 16:30"
