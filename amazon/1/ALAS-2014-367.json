{
  "id": "ALAS-2014-367",
  "title": "Amazon Linux AMI 2012.09 - ALAS-2014-367: medium priority package update for php54",
  "issued": {
    "date": "2014-07-09 16:24"
  },
  "updated": {
    "date": "2014-09-19 10:32"
