{
  "id": "ALAS2-2023-2027",
  "title": "Amazon Linux 2 2017.12 - ALAS2-2023-2027: important priority package update for kernel",
  "issued": {
    "date": "2023-04-27 18:37"
  },
  "updated": {
    "date": "2023-08-03 18:09"
