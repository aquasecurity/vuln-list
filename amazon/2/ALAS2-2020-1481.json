{
  "id": "ALAS2-2020-1481",
  "title": "Amazon Linux 2 2017.12 - ALAS2-2020-1481: medium priority package update for poppler",
  "issued": {
    "date": "2020-08-18 20:33"
  },
  "updated": {
    "date": "2020-08-24 23:54"
