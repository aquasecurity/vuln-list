{
  "id": "ALAS2-2024-2615",
  "title": "Amazon Linux 2 2017.12 - ALAS2-2024-2615: important priority package update for kernel",
  "issued": {
    "date": "2024-08-01 03:01:00"
  },
  "updated": {
    "date": "2025-05-19 23:18:00"
