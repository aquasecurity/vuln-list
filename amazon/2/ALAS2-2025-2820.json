{
  "id": "ALAS2-2025-2820",
  "title": "Amazon Linux 2 2017.12 - ALAS2-2025-2820: important priority package update for ghostscript",
  "issued": {
    "date": "2025-04-09 19:45:00"
  },
  "updated": {
    "date": "2025-04-16 12:29:00"
